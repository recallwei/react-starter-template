  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@brucesong/eslint-config-react": "^1.0.4",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.47.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  },
  "private": true,
  "license": "MIT"
}
