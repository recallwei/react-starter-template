{
  "name": "react-ts-starter-template",
  "description": "⚛️ An opinionated React/TypeScript starter template.",
  "author": "Bruce Song <recall4056@gmail.com> (https://github.com/recallwei/)",
  "scripts": {
    "dev": "vite",
    "dev:docker": "docker-compose up -d",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "cspell:check": "cspell \"**\"",
    "eslint:check": "eslint \"**/*.{ts,tsx}\" --color",
    "eslint:fix": "pnpm eslint:check --fix",
    "prettier:check": "prettier --check --ignore-unknown .",
    "prettier:fix": "prettier --write --ignore-unknown .",
    "type:check": "tsc",
    "cz": "git-cz",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.17.2",
    "@ant-design/happy-work-theme": "^1.0.0",
    "@hookform/resolvers": "^3.3.2",
    "@tanstack/react-query": "^5.4.3",
    "@tanstack/react-query-devtools": "^5.4.3",
    "ahooks": "^3.7.8",
    "antd": "^5.10.3",
    "axios": "^1.6.0",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.10",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-router-dom": "^6.18.0",
    "yup": "^1.3.2",
    "zustand": "^4.4.5"
  },
  "devDependencies": {
    "@brucesong/eslint-config-react": "^1.0.15",
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@hookform/devtools": "^4.3.1",
    "@iconify/json": "^2.2.135",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@types/nprogress": "^0.2.2",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@vitejs/plugin-react-swc": "^3.4.1",
    "autoprefixer": "^10.4.16",
    "commitizen": "^4.3.0",
    "cspell": "^7.3.8",
    "cz-git": "^1.7.1",
    "eslint": "^8.52.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.2.2",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-icons": "^0.17.1",
    "vite": "^4.5.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "private": true,
  "license": "MIT"
}
