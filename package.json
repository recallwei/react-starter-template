{
  "name": "react-ts-starter-template",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "cspell:check": "cspell \"**\"",
