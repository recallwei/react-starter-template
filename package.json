{
  "name": "react-ts-starter-template",
  "description": "⚛️ An opinionated React/TypeScript starter template.",
  "author": "Bruce Song <recall4056@gmail.com> (https://github.com/recallwei/)",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "cspell:check": "cspell \"**\"",
    "eslint:check": "eslint \"**/*.{ts,tsx}\" --color",
    "eslint:fix": "pnpm eslint:check --fix",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --check --write --ignore-unknown .",
    "type:check": "tsc",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "dayjs": "^1.11.9",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@brucesong/eslint-config-react": "^1.0.7",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "cspell": "^7.0.1",
    "eslint": "^8.47.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.2",
    "sass": "^1.66.1",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.16.6",
    "vite": "^4.4.9"
  },
  "packageManager": "pnpm@8.5.0",
  "engines": {
    "node": ">=18.16.0",
    "npm": ">=9.2.0"
  },
  "private": true,
  "license": "MIT"
}
