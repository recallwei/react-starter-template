{
  "name": "react-ts-starter-template",
  "description": "⚛️ An opinionated React/TypeScript starter template.",
  "author": "Bruce Song <recall4056@gmail.com> (https://github.com/recallwei/)",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:docker": "docker-compose up -d",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "cspell:check": "cspell \"**\"",
    "eslint:check": "eslint \"**/*.{ts,tsx}\" --color",
    "eslint:fix": "pnpm eslint:check --fix",
    "prettier:check": "prettier --check --ignore-unknown .",
    "prettier:fix": "prettier --write --ignore-unknown .",
    "type:check": "tsc",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.17.0",
    "@hookform/resolvers": "^3.3.1",
    "@tanstack/react-query": "^4.33.0",
    "@tanstack/react-query-devtools": "^4.33.0",
    "ahooks": "^3.7.8",
    "antd": "^5.8.6",
    "axios": "^1.5.0",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.9",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.46.1",
    "react-router-dom": "^6.15.0",
    "yup": "^1.2.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@brucesong/eslint-config-react": "^1.0.8",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@iconify/json": "^2.2.111",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@tanstack/eslint-plugin-query": "^4.34.1",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.15",
    "cspell": "^7.3.2",
    "eslint": "^8.48.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "sass": "^1.66.1",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.1.6",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-icons": "^0.17.0",
    "vite": "^4.4.9"
  },
  "private": true,
